* {
  padding: 0
  margin: 0
  box-sizing: border-box
}

body {
  background: #e6e6e6
}

.fix-container-app
  &::-webkit-scrollbar 
    width: 8px
  &::-webkit-scrollbar-thumb
    background-color: #fff
    border-radius: 10px
  &::-webkit-scrollbar-track
    background-color: #EB310CBF

button {
  cursor pointer
}

a {
  text-decoration: none
}

ul {
  list-style none
}